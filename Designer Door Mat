import math

s=input().split()
n=int(s[0])
m=int(s[1])
c='.|.'
w='WELCOME'
#above welcome
for i in range(0,math.floor(n/2 )):
    print((c*(2*i+1)).center(m,'-'))
#welcome
print(w.center(m,'-'))
#below welcome
for i in range(1,math.floor(n/2)+1):
    print((c*((2*math.floor(n/2)+1)-2*i)).center(m,'-'))
